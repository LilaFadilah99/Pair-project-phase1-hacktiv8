              </div>
              <div class="col-2">
                <a class="btn px-5 py-3 rounded-pill btn-outline-secondary" href="#" role="button">Fashion</a>
              </div>
              <div class="col-2">
                <a class="btn px-5 py-3 rounded-pill btn-outline-secondary" href="#" role="button">Fashion</a>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!--End Category  -->
      <!-- Search -->
      <div class="container my-5">
        <div class="container-fluid">
          <div class="col-5">
            <form class="d-flex" role="search" action="/" method="get">
              <h6><i class="fa-solid fa-magnifying-glass pt-3 me-2"></i></h6>
              <input class="form-control me-2 py-2" type="search" placeholder="Search" aria-label="Search" name="name" style="border-radius: 30px" />
              <!-- <button class="btn btn-outline-success" type="submit">Search</button> -->
            </form>
          </div>
        </div>
      </div>
      <!-- End Search -->
      <!-- Products Card -->
      <section id="productsCard">
        <div class="container-fluid my-5">
          <div class="container">
            <div class="row justify-content-center">
              <!-- card 1 -->
              <div class="mb-3 col-md-3 col-6">
                <div class="card h-100 rounded-2 shadow-sm poductsCard">
                  <div class="inner">
                    <img src="./assets/trusted.png" class="card-img-top" alt="paket 1" />
                  </div>
                  <div class="card-body">
                    <p class="card-title fw-bold">{products.title}</p>
                    <p class="card-text" style="font-size: 15px">Price</p>
                    <p style="font-size: 12px"></p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- Products Card -->
    </section>
    <!-- closing -->
    <section style="background-color: #13d0dc; padding: 100px 0px">
      <div class="container">
        <div class="container-fluid">
          <div class="row justify-content-center">
            <div class="col-8">
              <h1>Start Shopping Now</h1>
            </div>
            <div class="col-2">
              <a class="btn px-5 py-3" href="#" role="button" style="background-color: #00ffc4">Sign Up</a>
            </div>
